center=True, min_periods=1).mean()\n            \n            ax.plot(\n                df_filt['Time (MJD)'], rolling_flux, \n                color=FILTER_COLORS[filt], linewidth=2, label=f'Filtro {filt} (Suavizado)'\n            )\n\n    ax.set_title(f\"Objeto: {object_id} (Target: {meta['target']})\\nSuavizado vs. Crudo\")\n    ax.set_xlabel(\"Time (MJD)\")\n    ax.set_ylabel(\"Flux\")\n    ax.axhline(0, color='white', linestyle='--', alpha=0.3) # Línea de referencia 0\n    ax.legend()\n    plt.show()\n\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-01-13T04:37:12.463272Z","iopub.execute_input":"2026-01-13T04:37:12.463656Z","iopub.status.idle":"2026-01-13T04:37:12.471089Z","shell.execute_reply.started":"2026-01-13T04:37:12.463629Z","shell.execute_reply":"2026-01-13T04:37:12.470290Z"}},"outputs":[],"execution_count":5},{"cell_type":"code","source":"# PRUEBA ESTO CON EL OBJETO QUE TIENE EL PUNTO RARO\nplot_smoothed_lightcurve(tde_example, df_log, DATA_PATH, window_size=3)","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-01-13T04:37:12.605235Z","iopub.execute_input":"2026-01-13T04:37:12.605778Z","iopub.status.idle":"2026-01-13T04:37:12.659464Z","shell.execute_reply.started":"2026-01-13T04:37:12.605752Z","shell.execute_reply":"2026-01-13T04:37:12.658495Z"}},"outputs":[{"traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mNameError\u001b[0m                                 Traceback (most recent call last)","\u001b[0;32m/tmp/ipykernel_55/2740768125.py\u001b[0m in \u001b[0;36m<cell line: 0>\u001b[0;34m()\u001b[0m\n\u001b[1;32m      1\u001b[0m \u001b[0;31m# PRUEBA ESTO CON EL OBJETO QUE TIENE EL PUNTO RARO\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0;32m----> 2\u001b[0;31m \u001b[0mplot_smoothed_lightcurve\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0mtde_example\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mdf_log\u001b[0m\u001b[0;34m,\u001b[0m \u0