s():\n        #fitzpatrick99 returns and array, so we take value 0\n        a_lambda_map[filt] = fitzpatrick99(wl, ebv * 3.1)[0]\n\n    df['A_lambda'] = df['Filter'].map(a_lambda_map)\n    #Lets apply the correction formule F_ext = F * 10^(A_lambda / 2.5)\n    df['Flux_corrected'] = df['Flux'] * (10 ** (df['A_lambda'] / 2.5))\n\n    return df","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-01-13T04:37:06.805001Z","iopub.execute_input":"2026-01-13T04:37:06.805326Z","iopub.status.idle":"2026-01-13T04:37:06.816923Z","shell.execute_reply.started":"2026-01-13T04:37:06.805303Z","shell.execute_reply":"2026-01-13T04:37:06.815933Z"}},"outputs":[{"traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mModuleNotFoundError\u001b[0m                       Traceback (most recent call last)","\u001b[0;32m/tmp/ipykernel_55/279535031.py\u001b[0m in \u001b[0;36m<cell line: 0>\u001b[0;34m()\u001b[0m\n\u001b[0;32m----> 1\u001b[0;31m \u001b[0;32mfrom\u001b[0m \u001b[0mextinction\u001b[0m \u001b[0;32mimport\u001b[0m \u001b[0mfitzpatrick99\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0m\u001b[1;32m      2\u001b[0m \u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m      3\u001b[0m \u001b[0;31m#Physic constant\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m      4\u001b[0m WAVELENGHTS = {\n\u001b[1;32m      5\u001b[0m     \u001b[0;34m'u'\u001b[0m\u001b[0;34m:\u001b[0m \u001b[0mnp\u001b[0m\u001b[0;34m.\u001b[0m\u001b[0marray\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0;34m[\u001b[0m\u001b[0;36m3641\u001b[0m\u001b[0;34m]\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0;34m'g'\u001b[0m\u001b[0;34m:\u001b[0m \u001b[0mnp\u001b[0m\u001b[0;34m.\u001b[0m\u001b[0marray\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0;34m[\u001b[0m\u001b[0;36m4704\u001b[0m\u001b[0;34m]\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0;34m'r'\u001b[0m\u001b[0;34m:\u001b[0m\u001b[0m