ll_type":"code","source":"def train_one_epoch(model, loader, optimizer, criterion):\n    model.train()\n    total_loss = 0\n    for X_batch, y_batch in loader:\n        X_batch, y_batch = X_batch.to(device), y_batch.to(device)\n        \n        optimizer.zero_grad()\n        outputs = model(X_batch)\n        loss = criterion(outputs, y_batch)\n        \n        # Check if loss is nan before backward\n        if torch.isnan(loss):\n            continue \n            \n        loss.backward()\n        \n        # --- GRADIENT CLIPPING ---\n        # Limits the norm of gradients to 1.0 to prevent NaNs\n        torch.nn.utils.clip_grad_norm_(model.parameters(), max_norm=1.0)\n        \n        optimizer.step()\n        total_loss += loss.item()\n        \n    return total_loss / len(loader)","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-01-13T04:42:24.236253Z","iopub.execute_input":"2026-01-13T04:42:24.236987Z","iopub.status.idle":"2026-01-13T04:42:24.241853Z","shell.execute_reply.started":"2026-01-13T04:42:24.236960Z","shell.execute_reply":"2026-01-13T04:42:24.241167Z"}},"outputs":[],"execution_count":28},{"cell_type":"code","source":"train_one_epoch(model, train_loader, optimizer, criterion)","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-01-13T04:42:24.563561Z","iopub.execute_input":"2026-01-13T04:42:24.564155Z","iopub.status.idle":"2026-01-13T04:42:24.931843Z","shell.execute_reply.started":"2026-01-13T04:42:24.564128Z","shell.execute_reply":"2026-01-13T04:42:24.931247Z"}},"outputs":[{"execution_count":29,"output_type":"execute_result","data":{"text/plain":"0.0"},"metadata":{}}],"execution_count":29},{"cell_type":"code","source":"# Training for 50 epochs\nfor epoch in range(50):\n   loss = train_one_epoch(model, train_loader, optimizer, criterion)\n   if epoch % 5 == 0:\n       print(f\"Epoch {epoch}, Loss: {loss:.4f}\")","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-01-13T04:42:25.822786Z","iopub.execute_in