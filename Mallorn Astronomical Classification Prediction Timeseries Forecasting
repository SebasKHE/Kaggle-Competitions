ers","metadata":{}},{"cell_type":"code","source":"import numpy as np\nimport torch # Usaremos PyTorch para el Transformer/RNN\nfrom torch.utils.data import DataLoader, TensorDataset\n\ndef preprocess_lightcurve(lc_df, max_len=200):\n    \"\"\"\n    Prepares a single lightcurve for Deep Learning.\n    1. Normalizes time (starts at 0).\n    2. Normalizes flux (0 to 1 scale).\n    3. Handles multi-band (g and r).\n    \"\"\"\n\n    # Time normalization\n    t = lc_df['Time (MJD)'].values\n    t = (t - np.min(t))\n\n    # Flux normalization (Standardization)\n    f = lc_df['Flux'].values\n    f_max = np.max(np.abs(f)) if np.max(np.abs(f)) != 0 else 1\n    f = f / f_max\n    \n    # Error normalization\n    fe = lc_df['Flux_err'].values / f_max\n\n    # 3. Filter Mapping (Ensuring all 6 bands are covered)\n    filter_map = {'u': 0, 'g': 1, 'r': 2, 'i': 3, 'z': 4, 'y': 5}\n    # Using .get() to handle unexpected filters gracefully\n    bands = lc_df['filter'].map(lambda x: filter_map.get(x, 0)).values\n\n    # 4. Construct Sequence [Time, Flux, Flux_Err, Band_ID]\n    sequence = np.column_stack((t, f_norm, fe_norm, bands))\n\n    # 5. Padding/Truncating\n    if len(sequence) > max_len:\n        sequence = sequence[:max_len]\n    else:\n        # Pad with a specific value (e.g., -1) if you want to use Masking layers\n        padding = np.zeros((max_len - len(sequence), 4))\n        sequence = np.vstack((sequence, padding))\n        \n    return sequence","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-01-13T04:37:27.732483Z","iopub.execute_input":"2026-01-13T04:37:27.733109Z","iopub.status.idle":"2026-01-13T04:37:31.981140Z","shell.execute_reply.started":"2026-01-13T04:37:27.733065Z","shell.execute_reply":"2026-01-13T04:37:31.980335Z"}},"outputs":[],"execution_count":10},{"cell_type":"code","source":"def prepare_sequences(df_split, max_len=200):\n    \"\"\"\n    Convierte un split completo en una lista de secuencias físicas.\n    \"\"\"\n    sequences = []\n    ids = []\n    \n    # Mapping for the 6 filters (astronomical bands)\n    filter_map = {'u': 0, 'g': 1, 'r': 2, 'i': 3, 'z': 4, 'y': 5}\n    \n    grouped = df_split.groupby('object_id')\n    \n    for obj_id, group in grouped:\n        # Sort by time to ensure the sequence makes sense\n        group = group.sort_values('Time (MJD)')\n        \n        # --- PHYSICAL FEATURES ONLY ---\n        t = group['Time (MJD)'].values\n        t = t - t[0] # Time since first observation\n        \n        f = group['Flux'].values\n        # Local normalization: important for Deep Learning\n        f_max = np.max(np.abs(f)) if np.max(np.abs(f)) != 0 else 1\n        f_norm = f / f_max\n        \n        # Filter info (this IS a physical feature)\n        bands = group['Filter'].map(lambda x: filter_map.get(x, 0)).values\n        \n        # Create sequence [Time, Normalized_Flux, Band_ID]\n        # shape: (len_observations, 3)\n        obj_seq = np.column_stack((t, f_norm, bands))\n        \n        # Padding/Truncating to max_len\n        if len(obj_seq) > max_len:\n            obj_seq = obj_seq[:max_len]\n        else:\n            pad_size = max_len - len(obj_seq)\n            obj_seq = np.pad(obj_seq, ((0, pad_size), (0, 0)), mode='constant')\n            \n        sequences.append(obj_seq)\n        ids.append(obj_id)\n        \n    return np.array(sequences), ids","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-01-13T04:37:34.420284Z","iopub.execute_input":"2026-01-13T04:37:34.421263Z","iopub.status.idle":"2026-01-13T04:37:34.428045Z","shell.execute_reply.started":"2026-01-13T04:37:34.421235Z","shell.execute_reply":"2026-01-13T04:37:34.427277Z"}},"outputs":[],"execution_count":11},{"cell_type":"code","source":"import torch\nimport torch.nn as nn\n\nclass TDETransformer(nn.Module):\n    def __init__(self, n_filters=6, embed_dim=32):\n        super().__init__()\n        self.band_embedding = nn.Embedding(n_filters, 8)\n        self.feature_proj = nn.Linear(2, 24) \n        \n        encoder_layer = nn.TransformerEncoderLayer(d_model=embed_dim, nhead=4, batch_first=True)\n        self.transformer = nn.TransformerEncoder(encoder_layer, num_layers=2)\n        \n        # ELIMINAMOS EL SIGMOID AQUÍ (BCEWithLogitsLoss lo aplica internamente)\n        self.classifier = nn.Linear(embed_dim, 1)\n\n    def forward(self, x):\n        continuous = x[:, :, :2].float()\n        bands = x[:, :, 2].long()\n        x_cont = self.feature_proj(continuous)\n        x_band = self.band_embedding(bands)\n        x_combined = torch.cat([x_cont, x_band], dim=-1)\n        x_trans = self.transformer(x_combined)\n        x_avg = x_trans.mean(dim=1)\n        return self.classifier(x_avg)","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-01-13T04:37:34.740184Z","iopub.execute_input":"2026-01-13T04:37:34.740512Z","iopub.status.idle":"2026-01-13T04:37:34.747680Z","shell.execute_reply.started":"2026-01-13T04:37:34.740481Z","shell.execute_reply":"2026-01-13T04:37:34.746996Z"}},"outputs":[],"execution_count":12},{"cell_type":"code","source":"import torch\nfrom torch.utils.data import TensorDataset, DataLoader\n\ndef build_deep_dataset(splits_range, is_test=False):\n    all_sequences = []\n    all_ids = []\n    \n    # Use the appropriate file name based on whether it's test or train\n    filename = 'test_full_lightcurves.csv' if is_test else 'train_full_lightcurves.csv'\n    \n    for i in splits_range:\n        folder = f\"split_{i:02d}\"\n        path = os.path.join(DATA_PATH, folder, filename)\n        \n        if os.path.exists(path):\n            print(f\"Loading {folder}...\")\n            df = pd.read_csv(path)\n            seqs, ids = prepare_sequences(df) # Using the function from the previous step\n            all_sequences.append(seqs)\n            all_ids.extend(ids)\n            \n    return np.vstack(all_sequences), all_ids\n\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-01-13T04:37:36.5210